<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="workflow_oehealth_professional" model="workflow">
            <field name="name">oehealth.professional.wkf</field>
            <field name="osv">oehealth.professional</field>
            <field name="on_create" eval='True' />
        </record>

         <record id="act_new" model="workflow.activity">
            <field name="wkf_id" ref="workflow_oehealth_professional" />
            <field name="flow_start" eval='True' />
            <field name="name">professional_new</field>
            <field name="kind">function</field>
            <field name="action">oehealth_professional_new()</field>
        </record>

        <record id="act_revised" model="workflow.activity">
            <field name="wkf_id" ref="workflow_oehealth_professional" />
            <field name="name">professional_revised</field>
            <field name="kind">function</field>
            <field name="action">oehealth_professional_revised()</field>
        </record>

        <record id="act_waiting" model="workflow.activity">
            <field name="wkf_id" ref="workflow_oehealth_professional" />
            <field name="name">professional_waiting</field>
            <field name="kind">function</field>
            <field name="action">oehealth_professional_waiting()</field>
        </record>

        <record id="act_okay" model="workflow.activity">
            <field name="wkf_id" ref="workflow_oehealth_professional" />
            <field name="name">professional_okay</field>
            <field name="kind">function</field>
            <field name="action">oehealth_professional_okay()</field>
        </record>

        <record id="t1" model="workflow.transition">
            <field name="act_from" ref="act_new" />
            <field name="act_to" ref="act_revised" />
            <field name="signal">oehealth_professional_revised</field>
        </record>

        <record id="t2" model="workflow.transition">
            <field name="act_from" ref="act_new" />
            <field name="act_to" ref="act_waiting" />
            <field name="signal">oehealth_professional_waiting</field>
        </record>

        <record id="t3" model="workflow.transition">
            <field name="act_from" ref="act_new" />
            <field name="act_to" ref="act_okay" />
            <field name="signal">oehealth_professional_okay</field>
        </record>

        <record id="t4" model="workflow.transition">
            <field name="act_from" ref="act_revised" />
            <field name="act_to" ref="act_waiting" />
            <field name="signal">oehealth_professional_waiting</field>
        </record>

        <record id="t5" model="workflow.transition">
            <field name="act_from" ref="act_revised" />
            <field name="act_to" ref="act_okay" />
            <field name="signal">oehealth_professional_okay</field>
        </record>

        <record id="t6" model="workflow.transition">
            <field name="act_from" ref="act_waiting" />
            <field name="act_to" ref="act_revised" />
            <field name="signal">oehealth_professional_revised</field>
        </record>

        <record id="t7" model="workflow.transition">
            <field name="act_from" ref="act_waiting" />
            <field name="act_to" ref="act_okay" />
            <field name="signal">oehealth_professional_okay</field>
        </record>

        <record id="t8" model="workflow.transition">
            <field name="act_from" ref="act_okay" />
            <field name="act_to" ref="act_revised" />
            <field name="signal">oehealth_professional_revised</field>
        </record>

    </data>
</openerp>

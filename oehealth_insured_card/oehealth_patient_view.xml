<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_oehealth_patient_form" model="ir.ui.view">
            <field name="name">oehealth.patient.form</field>
            <field name="model">oehealth.patient</field>
            <field name="inherit_id" ref="oehealth_patient.view_oehealth_patient_form"/>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <field name="patient_info" position="after">
                    <group string="Insured Cards">
                        <field name="insured_card_ids" colspan="4" nolabel="1" />
                    </group>
                </field>
            </field>
        </record>

    </data>
</openerp>
